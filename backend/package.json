{
  "name": "fullstack-ts-app-backend",
  "version": "1.0.0",
  "description": "Backend application for the Full-Stack TypeScript app using NestJS.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/main.js",
    "start:dev": "ts-node -r tsconfig-paths/register -r dotenv/config src/main.ts",
    "dev": "ts-node -r tsconfig-paths/register -r dotenv/config --transpile-only src/main.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "node -r ts-node/register prisma/seed.ts",
    "prepare": "prisma generate",
    "lint": "eslint . --ext .ts",
    "test": "pnpm test:unit",
    "test:unit": "jest --config jest.config.js",
  "test:e2e": "powershell -Command \"$env:JEST_E2E='true'; jest --config jest.e2e.config.js\"",
    "test:all": "pnpm test:unit && pnpm test:e2e"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.5",
    "@nestjs/core": "^10.3.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.16",
    "@prisma/client": "5.15.0",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "module-alias": "^2.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.0.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "socket.io": "^4.0.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/config": "2.3.4",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/platform-socket.io": "^10.3.5",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/websockets": "^10.3.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prisma": "5.15.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.0",
    "@nestjs/testing": "^10.3.5",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}